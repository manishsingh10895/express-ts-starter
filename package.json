{
    "name": "express-ts-starter",
    "version": "1.5.0",
    "private": true,
    "scripts": {
        "test": "mocha --timeout 5000 -r ts-node/register tests/**/*.ts ",
        "start:dev": "NODE_ENV=development npm run build && concurrently  \"tsc -w -p src\" nodemon --inspect --config \"./nodemon.json\"/",
        "start:dev:win": "pwsh start.ps1",
        "start:batman:dev": "NODE_ENV=localbatman npm run build && concurrently  \"tsc -w -p src\" nodemon --inspect --config \"./nodemon.json\"/",
        "dev": "nodemon --config \"./nodemon.json\"/",
        "start:sub": "NODE_ENV=development npm run build:sub && concurrently \"tsc -w -p subscribers\" \"nodemon --inspect --config ./nodemon-redis.json\"",
        "start:sub:win": "pwsh start-sub.ps1",
        "build": "rm -rf ./dist/ && tsc",
        "build:win": "pwsh ./build.ps1",
        "build:sub:win": "pwsh ./build-sub.ps1",
        "build:sub": "rm -rf ./distsubs/ && tsc -p subscribers",
        "start": "node dist/index.js",
        "start:batman": "NODE_ENV=batman node dist/index.js",
        "build-test": "rm -rf ./test && tsc -w -p tests"
    },
